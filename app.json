{
  "expo": {
    "name": "DinnerParty",
    "slug": "DinnerParty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/DP_app_icon.png",
    "userInterfaceStyle": "light",
    "scheme": "dinner-party",
    "splash": {
      "image": "./assets/images/DP_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iancarscadden.dinnerparty",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "077ccef8-6410-4b89-a548-b2e54357656c"
      }
    }
  }
}
